{
  "metadata": [
    {
      "src": [
        {
          "files": ["**.csproj"]
        }
      ],
      "dest": "./"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**.md", "**.yml", "toc.yml"],
        "src": "."
      }
    ],
    "dest": "_site",
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "_appTitle": "Frank.DataStorage Documentation"
    },
    "template": ["statictoc"],
    "postProcessors" : ["FixLinks", "FixBrokenLinks"]
  }
}
